\{{#each this}}
<div class="simple-list-item mix \{{data.[classes]}}" data-entry-id="\{{id}}">
  \{{#if data.[Image]}}
  <div class="list-item-image" style="background-image: url('\{{auth data.[Image]}}')"></div>
  \{{/if}}
  <div class="list-item-body">
    \{{#if data.[Title]}}
    <p class="list-item-title">\{{data.[Title]}}</p>
    \{{/if}}
    \{{#if data.[Description]}}
    <p class="list-item-description">\{{data.[Description]}}</p>
    \{{/if}}
    \{{#if data.[Category]}}
    <p class="list-item-subtitle">\{{data.[Category]}}</p>
    \{{/if}}
  </div>
</div>
\{{/each}}