\{{#each this}}
  <div class="agenda-list-day-holder">
    <div id="contentArea-\{{@index}}" class="agenda-list-card-holder">
      \{{#each this}}
      <div class="agenda-list-item" data-entry-id="\{{id}}">
        <div class="agenda-list-item-content">
          <div class="container-fluid slide-over session-container">
            <div class="agenda-item-inner-content clearfix">
              <div class="agenda-item-info-holder">
                <div class="agenda-item-time">\{{data.[Start Time]}}</div>
                <div class="agenda-item-time line">|</div>
                <div class="agenda-item-time">\{{data.[End Time]}}</div>
              </div>
              <div class="agenda-item-content-holder">
                <h2 class="agenda-item-title">\{{data.[Title]}}</h2>
                <div class="agenda-item-session-location">
                  <p class="agenda-item-location">\{{{data.[Location]}}}</p>
                  <p class="agenda-item-start-date-time">\{{data.[Start Time]}} - \{{data.[End Time]}} / \{{data.[Date]}}</p>
                </div>
                
                <div class="agenda-item-bookmark">
                  <div class="agenda-item-bookmark-holder agenda-item-bookmark-holder-\{{id}}">
                    <!-- Bookmark button will be here -->
                    <div class="agenda-item-add-agenda"></div>
                  </div>
                </div>
                
                <div class="agenda-item-description">
                  \{{{data.[Content]}}}
                </div>
              </div>
            </div>
          </div>
          <div class="agenda-item-close-btn"><i class="fa fa-times"></i></div>
        </div>
      </div>
      \{{/each}}
    </div>
    <div class="agenda-item-empty-state">Nothing bookmarked for this date</div>
  </div>
  \{{/each}}